
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt" android:drawable="@drawable/ic_launcher_foreground">
    <target android:name="logo">
        <aapt:attr name="android:animation">
            <set android:ordering="together">
                <objectAnimator
                    android:duration="1000"
                    android:propertyName="scaleX"
                    android:valueFrom="0.1"
                    android:valueTo="0.3540249"
                    android:valueType="floatType" >
                    <aapt:attr name="android:interpolator">
                        <pathInterpolator android:pathData="M 0,0 C 0.05, 0, 0.133333, 0.06, 0.166666, 0.4 C 0.208333, 0.82, 0.25, 1, 1, 1" />
                    </aapt:attr>
                </objectAnimator>
                <objectAnimator
                    android:duration="1000"
                    android:propertyName="pivotX"
                    android:valueFrom="54"
                    android:valueTo="27"
                    android:valueType="floatType" >
                    <aapt:attr name="android:interpolator">
                        <pathInterpolator android:pathData="M 0,0 C 0.05, 0, 0.133333, 0.06, 0.166666, 0.4 C 0.208333, 0.82, 0.25, 1, 1, 1" />
                    </aapt:attr>
                </objectAnimator>
                <objectAnimator
                    android:duration="1000"
                    android:propertyName="pivotY"
                    android:valueFrom="54"
                    android:valueTo="27"
                    android:valueType="floatType" >
                    <aapt:attr name="android:interpolator">
                        <pathInterpolator android:pathData="M 0,0 C 0.05, 0, 0.133333, 0.06, 0.166666, 0.4 C 0.208333, 0.82, 0.25, 1, 1, 1" />
                    </aapt:attr>
                </objectAnimator>
                <objectAnimator
                    android:duration="1000"
                    android:propertyName="scaleY"
                    android:valueFrom="0.1"
                    android:valueTo="0.3540249"
                    android:valueType="floatType" >
                    <aapt:attr name="android:interpolator">
                        <pathInterpolator android:pathData="M 0,0 C 0.05, 0, 0.133333, 0.06, 0.166666, 0.4 C 0.208333, 0.82, 0.25, 1, 1, 1" />
                    </aapt:attr>
                </objectAnimator>
            </set>
        </aapt:attr>
    </target>
</animated-vector>